<?xml version="1.0"?>
<launch>

    <arg name="uavId" default="0"/>
    <arg name="altitude" default="8.0"/>
    <arg name="taskTypes" default="[0,1,2]"/>
    <arg name="workerAssignAtSeconds" default="5"/>
    <arg name="serialPort" default="/dev/ttyUSB0"/>
    <arg name="baud" default="57600"/>

    <node pkg="multi_uav_se_mission" name="worker_node" type="worker_node" output="screen">
        <!-- parameters -->
        <param name="uavId" value="$(arg uavId)" />
        <param name="altitude" value="$(arg altitude)" />
        <param name="taskTypes" value="$(arg taskTypes)" />
        <param name="workerAssignAtSeconds" value="$(arg workerAssignAtSeconds)" />
        <param name="serialPort" value="$(arg serialPort)" />
        <param name="baud" value="$(arg baud)" />
    </node>

</launch>
